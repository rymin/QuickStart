<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet author="erymin" id="addVehicleTable">
        <createTable tableName="vehicle">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="brand" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="model" type="varchar(255)">
                <constraints nullable="false" />
            </column>
        </createTable>
    </changeSet>

    <changeSet author="erymin" id="addTestData">
        <insert tableName="vehicle">
            <column name="id" value="10"/>
            <column name="brand" value="bmw"/>
            <column name="model" value="x3"/>
        </insert>

        <insert tableName="vehicle">
            <column name="id" value="20"/>
            <column name="brand" value="mercedes"/>
            <column name="model" value="c230"/>
        </insert>
    </changeSet>

    <changeSet  author="erymin"  id="createSequence">
        <createSequence incrementBy="10" schemaName="public" sequenceName="car_id_seq" startValue="100"/>
    </changeSet>
</databaseChangeLog>